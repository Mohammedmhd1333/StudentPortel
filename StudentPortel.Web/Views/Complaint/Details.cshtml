@model StudentPortel.Web.Models.Entities.Addcomplaint

@{
    ViewData["Title"] = "Complaint Details";

}

<h1>@ViewData["Title"]</h1>

<div>
    @* <h4>Complaint Details</h4> *@
    <dl class="row">
        <dt class="col-sm-2">Created Date</dt>
        <dd class="col-sm-10">@Model.CreatedDate.ToString("MMM dd, yyyy")</dd>

        <dt class="col-sm-2">Employee Id</dt>
        <dd class="col-sm-10">@Model.EmployeeId</dd>

        <dt class="col-sm-2">Employee Name</dt>
        <dd class="col-sm-10">@Model.EmployeeName</dd>

        <dt class="col-sm-2">Department</dt>
        <dd class="col-sm-10">@Model.Department</dd>

        <dt class="col-sm-2">Work Location</dt>
        <dd class="col-sm-10">@Model.WorkLocation</dd>

        <dt class="col-sm-2">Suggestions</dt>
        <dd class="col-sm-10">@Model.suggestions</dd>

        <dt class="col-sm-2">Status</dt>
        <dd class="col-sm-10">@Model.Status</dd>

        <dt class="col-sm-2">Remark</dt>
        <dd class="col-sm-10">@Model.Remark</dd>
    </dl>
</div>

<a href="@Url.Action("Edit", new { id = Model.Id })" class="btn btn-warning">Edit</a>

<a href="@Url.Action("List")" class="btn btn-secondary">Back to List</a>
